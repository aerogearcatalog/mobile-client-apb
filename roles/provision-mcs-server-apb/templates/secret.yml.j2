apiVersion: v1
kind: Secret
metadata:
  name: {{ mobile_client_service_secret_name }}
  namespace: {{ namespace }}
  labels:
    name: mcs-server
    mobile: enabled
    serviceName: mcs-server
    serviceInstanceID: {{ _apb_service_instance_id }}
stringData:
  type: "{{ mobile_client_service_secret_name }}"
  name: "{{ mobile_client_service_secret_name }}"
  uri: "{{ mobile_client_service_protocol}}://{{ mobile_client_service_route.route.spec.host }}"
